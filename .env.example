# ==============================
# Discord Bot Configuration
# ==============================

# Your Discord bot token (keep this secret!)
DISCORD_TOKEN=your-bot-token-here

# Bot prefix (for legacy message commands)
PREFIX=!

# Lavalink Node(s)
LAVALINK_HOST=localhost
LAVALINK_PORT=2333
LAVALINK_PASSWORD=your-lavalink-password
LAVALINK_IDENTIFIER=LocalhostNode

# Server & Channel IDs
GUILD_ID=your-server-id
LOGGER_CHANNEL_ID=channel-id-for-uptime-logs
STAFF_CHANNEL_ID=channel-id-for-staff-logs

# Bot Presence
STATUS_NAME=Moozik is live!
STATUS_TYPE=LISTENING

# Express server (if using for uptime monitoring)
PORT=3000
